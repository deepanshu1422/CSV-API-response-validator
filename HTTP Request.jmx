              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">FileWriter fWriter = new FileWriter(&quot;/Users/deepanshuudhwani/Downloads/seo/2.csv&quot;, true);
BufferedWriter buff = new BufferedWriter(fWriter);

buff.write(vars.get(&quot;url&quot;) + &quot;,&quot; + vars.get(&quot;meta_title&quot;) + &quot;,&quot; + vars.get(&quot;meta_description&quot;) + &quot;,&quot; + vars.get(&quot;meta_keywords&quot;) + &quot;,&quot; + vars.get(&quot;meta_breadcrumb_title&quot;) + &quot;,&quot; + vars.get(&quot;q_filter&quot;) + &quot;\n&quot; );


